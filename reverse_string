#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N 10
// Function prototypes
void reverse_string(char *s);

// Main Function

int main(){
    char array[N] = "Tamra";
    int i;
    // reverse string function
    reverse_string(array);
    fprintf(stdout,"%s", array);
    return (EXIT_SUCCESS);
}

void reverse_string(char *s){
    int i, j;
    char tmp;
    i = 0;
    j = strlen(s)-1;
    while (i<j){
        if(s[i]!=s[j]){
            tmp = s[i];
            s[i] = s[j];
            s[j] = tmp;
        }
        i++;
        j--;
    }
}
