#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>
#define N 10
void subsequence (int *a, int n);
int main(void){
    int n,i;
    int array[N];
    fprintf(stdout,"insert size of array:\n");
    scanf("%d", &n);

    fprintf(stdout,"insert array:\n");
    for(i=0;i<n;i++){
        scanf("%d", &array[i]);
    }
    // function call
    subsequence(array,n);

    return (EXIT_SUCCESS);
}

void subsequence (int *a, int n){
    int i, j, k, h;
    int found, toPrint;
    found = 0;
    for(i=n; !found && i>0;i--){
        for(j=0;j<=n-i;j++){
            toPrint = 1;
            // check condition
            for(k=j; toPrint && k<i+j;k++){
                if(a[k] == 0){
                    toPrint = 0;
                }
            }
            if(toPrint){
                found = 1;
                fprintf(stdout,"maximum subsequence:\n");
                for(h=j;h<i+j;h++){
                    fprintf(stdout,"%d", a[h]);
                }
                fprintf(stdout,"\n");
                j = k+1;
            }

        }

    }


}
