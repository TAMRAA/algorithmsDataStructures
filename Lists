#include <stdio.h>
#include <stdlib.h>
#include <string.h>

// single node of a single linked list
typedef struct{
    int data;
    struct node *link;
}node_t;

int main(void){
    node_t *head;
    head = (node_t *) malloc (sizeof (node_t));

    if(head==NULL){
        fprintf(stderr,"memory allocation error");
        exit(EXIT_FAILURE);
    }

    head->data = 45;
    head->link = NULL;

    fprintf(stdout,"%d", head->data);
    return (EXIT_SUCCESS);

}


// single linked list
typedef struct node {
    int data;
    struct node *link;
}node;

int main(void){
    node *head = malloc(sizeof (node));
    if(head == NULL){
        fprintf(stderr,"memory alocation failed");
        exit(EXIT_FAILURE);
    }
    head->data = 45;
    head->link = NULL;

    node  *current = malloc(sizeof (node));
    if(current == NULL){
        fprintf(stderr,"memory alocation failed");
        exit(EXIT_FAILURE);
    }
    current->data = 93;
    current->link = NULL;

    head->link = current;

    current = malloc(sizeof (node));
    if(current == NULL){
        fprintf(stderr,"memory alocation failed");
        exit(EXIT_FAILURE);
    }

    current->data = 100;
    current->link = NULL;

    head->link->link =current;
    return 0;

}

