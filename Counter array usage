#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N 100

// Function prototypes
void countPerTens (int data[], int n);


// Main function
int main(void) {
    int array[N],array1[N];
    int dim, i;
    fprintf(stdout,"insert dim of the array:\n");
    scanf("%d", &dim);
    fprintf(stdout,"insert array:\n");
    for(i=0;i<dim;i++){
        array1[i] = 0;
    }
    for(i=0;i<dim;i++){
        scanf("%d", &array[i]);
    }
    countPerTens(array,dim);

    return (EXIT_SUCCESS);
}

void countPerTens (int data[], int n){
    int i, d, count[10];
    for(i=0;i<10;i++){
        count[i] = 0;
    }

    for(i=0;i<n;i++){
        d = data[i] / 10;
        count[d] = count[d] + 1;
    }

    for(i=0 ; i < 10; i++){
        fprintf(stdout,"%d element in range of (%d)\n", count[i], i);
    }
}
