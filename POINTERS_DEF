#include <stdio.h>

//                                                  POINTER DEFINITION

struct student {
char s1[L], s2[L];
int i;
float f;
};


void swap (int *x, int *y);


int main(){

    int i = 10;
    int j = 20;
    int *p1;
    int *p2;
    p1 = &i;
    p2 = &j;

    if(*p1 == *p2){
        printf("same value\n");
    }else{
        printf("not the same value\n");
    }

    if(p1==p2){
        printf("same location\n");
    }else{
        printf("different location\n");
    }

    if(p1 > p2){
        printf("p1 is after p2 in the memory\n");
    }else{
        printf("p1 is before p2 in the memory\n");
    }
    swap(p1,p2);
    printf("%d %d", *p1,*p2);
}

void swap (int *x, int *y) {
    int tmp;
    tmp = *x;
    *x = *y;
    *y = tmp;
    return;
}

void my_read (struct student *v) {
char s1[DIM], s2[DIM];
int i; float f;
fprintf (stdout, â€œ...: ");
scanf ("%s%s%d%d", s1, s2, &i, &f);
strcpy (v->s1, s1);
strcpy (v->s2, s2);
v->i = i;
v->f = f;
return;
}

