int main(){
    char string[N] = "this 12345 is a string";
    string_eliminator(string);
    printf("%s",string);
    return 0;
}
void string_eliminator(char *str){
    int i, j;
    i = 0;
    while (i< strlen(str)){
        if(str[i] == ' ' || str[i]>='0' && str[i]<='9'){
            for(j=i+1;j< strlen(str) +1 ; j++){
                str[j-1] = str[j];
            }
        }else{
            i = i + 1;
        }
    }
    return;
}
