#include "stdio.h"
#include "string.h"
#define N 100
// > <
void swap_values (int *x, int *y);
int main(){
    int a = 2, b = 3;
    // function call
    swap_values(&a, &b);
    printf("a: %d \nb: %d", a, b);
    return 0;

}
void swap_values (int *x, int *y){
    int tmp;
    tmp = *x;
    *x = *y;
    *y = tmp;
    return;
}
