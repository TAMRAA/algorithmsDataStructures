#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#define N 100
void swap_string(char v1[], char v2[]);
int main(void){
    char string1[N] = "Abdallah";
    char string2[N] = "Tamra";

    swap_string(string1,string2);
    fprintf(stdout,"%s %s", string1, string2);

    return (EXIT_SUCCESS);
}
void swap_string(char v1[], char v2[]){
    char *tmp;
    tmp = strdup(v1);
    strcpy(v1,v2);
    strcpy(v2,tmp);
}
