#include "stdio.h"
#include "string.h"
#define N 10
// > <
void swap_string(char *string1, char *string2);
int main(){
    char str1[N], str2[N];
    printf("insert first string:\n");
    scanf("%s", str1);
    printf("insert second string:\n");
    scanf("%s", str2);
    // function call
    swap_string(str1,str2);
    printf("first string: %s\n", str1);
    printf("second string: %s", str2);
    return 0;
}
void swap_string(char *string1, char *string2){
    char *tmp[N];
    strcpy(tmp,string1);
    strcpy(string1,string2);
    strcpy(string2,tmp);
}
