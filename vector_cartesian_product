#include <stdio.h>
#define D1 3
#define D2 4
void prodCartFilt(int *v1, int d1, int *v2, int d2) ;
int main(void){
    int v1[D1] = {1,2,3};
    int v2[D2] = {3,4,5,6};
    prodCartFilt(v1,D1,v2,D2);
    return 0;
}
void prodCartFilt(int *v1, int d1, int *v2, int d2){
    int i, j;
    for(i=0;i<d1;i++){
        for(j=0;j<d2;j++){
            if(v1[i]!=v2[j] && v1[i]-v2[j] != 1 && v1[i]-v2[j] != -1){
                printf("(%d,%d)", v1[i], v2[j]);
            }
        }
    }
    return;
}
